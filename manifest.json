{
  "name": "ViTwo",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "ViTwo",
  "action": {
    "default_title": "ViTwo",
    "default_popup": "popup.html"    
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.7.1.min.js", "myscript.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}